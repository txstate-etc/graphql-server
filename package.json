{
  "name": "@txstate-mws/graphql-server",
  "version": "2.2.2",
  "description": "A simple graphql server designed to work with typegraphql.",
  "exports": {
    "types": "./dist-esm/index.d.ts",
    "require": "./dist/index.js",
    "import": "./dist-esm/index.js"
  },
  "types": "./dist-esm/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rm -rf dist && tsc && cp src/*.html ./dist/ && mv dist/*.d.ts dist-esm/",
    "mocha": "TS_NODE_PROJECT=test/tsconfig.json mocha -r ts-node/register test/**/*.ts --exit",
    "test": "./test.sh",
    "start": "ts-node testservice/index.ts"
  },
  "dependencies": {
    "@graphql-tools/utils": "^8.0.1",
    "dataloader-factory": "^4.1.3",
    "fastify-txstate": "^3.0.11",
    "graphql": "^15.5.0",
    "jose": "^5.0.2",
    "lru-cache": "^10.0.1",
    "reflect-metadata": "^0.2.1",
    "txstate-utils": "^1.3.2",
    "type-graphql": "^1.2.0-rc.1"
  },
  "devDependencies": {
    "@apollo/gateway": "^0.50.0",
    "@types/chai": "^4.2.15",
    "@types/lru-cache": "^7.10.10",
    "@types/mocha": "^10.0.0",
    "@types/node": "^16.4.9",
    "@types/validator": "^13.7.1",
    "apollo-server": "^3.1.2",
    "axios": "^1.6.0",
    "chai": "^4.2.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "mercurius": "^9.0.0",
    "mocha": "^10.0.0",
    "ts-node": "^10.1.0",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/txstate-etc/graphql-server.git"
  },
  "keywords": [
    "graphql",
    "typegraphql",
    "fastify"
  ],
